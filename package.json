{
	"name": "msr-register-form",
	"version": "0.1.0",
	"private": false,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "NODE_OPTIONS='-r dotenv/config --loader newrelic/esm-loader.mjs -r newrelic' next start",
		"start:ci": "next start",
		"lint": "next lint",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"prepare": "husky",
		"test": "vitest -c ./vitest.config.unit.mjs",
		"test:int": "vitest -c ./vitest.config.integration.mjs",
		"test:unit:ci": "vitest run -c ./vitest.config.unit.mjs --coverage",
		"test:int:ci": "vitest run -c ./vitest.config.integration.mjs --coverage",
		"db:push": "prisma db push --schema prisma/postgres/schema.prisma",
		"mongodb:push": "prisma db push --schema prisma/mongodb/schema.prisma",
		"e2e": "cypress open",
		"e2e:ci": "cypress run --browser chrome",
		"postgres:generate": "prisma generate --schema prisma/postgres/schema.prisma",
		"generate": "prisma generate --schema prisma/postgres/schema.prisma & prisma generate --schema prisma/mongodb/schema.prisma",
		"mongodb:generate": "prisma generate --schema prisma/mongodb/schema.prisma",
		"postinstall": "prisma generate --schema prisma/postgres/schema.prisma & prisma generate --schema prisma/mongodb/schema.prisma",
		"db:seed": "node seed.mjs"
	},
	"dependencies": {
		"@next/third-parties": "^14.2.5",
		"@prisma/client": "^5.16.1",
		"@radix-ui/react-checkbox": "^1.1.1",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-visually-hidden": "^1.1.0",
		"@radix-ui/themes": "^3.1.1",
		"dotenv": "^16.4.5",
		"formik": "^2.4.6",
		"newrelic": "^12.4.0",
		"next": "14.2.4",
		"pino": "^9.4.0",
		"prisma": "^5.16.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-input-mask": "3.0.0-alpha.2",
		"react-select": "^5.8.0",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@prisma/nextjs-monorepo-workaround-plugin": "^5.20.0",
		"@testing-library/cypress": "^10.0.2",
		"@testing-library/dom": "^10.2.0",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/newrelic": "^9.14.4",
		"@types/node": "^20",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/react-input-mask": "^3.0.5",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitest/coverage-v8": "^1.6.0",
		"cypress": "^13.12.0",
		"cypress-axe": "^1.5.0",
		"eslint": "^8.57.0",
		"eslint-config-next": "14.2.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest-dom": "^5.4.0",
		"eslint-plugin-jsx-a11y": "^6.9.0",
		"eslint-plugin-testing-library": "^6.2.2",
		"husky": "^9.0.11",
		"jsdom": "^24.1.0",
		"lint-staged": "^15.2.7",
		"msw": "^2.3.5",
		"prettier": "^3.3.2",
		"typescript": "^5.2.2",
		"vite-tsconfig-paths": "^5.0.1",
		"vitest": "^1.6.0",
		"vitest-mock-extended": "^1.3.1"
	},
	"engines": {
		"node": ">=20.12.2",
		"pnpm": ">=9.2.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mapadoacolhimento/cadastro-msr.git"
	},
	"homepage": "https://github.com/mapadoacolhimento/cadastro-msr/#readme",
	"license": "CC-BY-SA-4.0",
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write --ignore-unknown"
		],
		"*.{md,html,css,cjs,json}": "prettier --write --ignore-unknown"
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 2,
		"semi": true,
		"singleQuote": false,
		"printWidth": 80,
		"useTabs": true
	},
	"author": {
		"name": "Desenvolvedoras Mapa",
		"email": "dev@mapa.org.br",
		"url": "https://mapadoacolhimento.org"
	},
	"description": "Formul√°rio de cadastro das acolhidas do Mapa do Acolhimento."
}
